{
  "schemaVersion": 1,
  "id": "tenshilib",
  "version": "${version}",
  "name": "TenshiLib",
  "description": "Core and library mod for most of my mods. Contains useful hooks and functions",
  "authors": [
    "Flemmli97"
  ],
  "contact": {
    "homepage": "${homepage}",
    "issues": "https://github.com/Flemmli97/TenshiLib/issues",
    "sources": "https://github.com/Flemmli97/TenshiLib"
  },
  "license": "ARR",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.flemmli97.tenshilib.fabric.TenshiLibFabric"
    ],
    "client": [
      "io.github.flemmli97.tenshilib.fabric.client.TenshiLibFabricClient"
    ]
  },
  "mixins": [
    "tenshilib.mixins.json",
    "tenshilib.fabric.mixins.json"
  ],
  "accessWidener": "tenshilib.accesswidener",
  "depends": {
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${mcversion}"
  }
}